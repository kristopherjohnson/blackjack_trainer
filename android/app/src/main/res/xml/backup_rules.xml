<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!--
    Since this app uses session-only data with no persistence,
    there is nothing to backup. All data is intentionally ephemeral.
    -->
    <exclude domain="database" />
    <exclude domain="sharedpref" />
    <exclude domain="external" />
</full-backup-content>